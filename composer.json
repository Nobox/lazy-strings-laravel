{
    "name": "nobox/lazy-strings-laravel",
    "description": "Laravel 5 service provider for LazyStrings.",
    "license" : "MIT",
    "keywords": ["laravel", "googledocs", "spreadsheet"],
    "homepage": "https://github.com/Nobox/lazy-strings-laravel",
    "authors": [
        {
            "name": "Alberto Estrada",
            "email": "alberto@nobox.com"
        },
        {
            "name": "Antonio Rodriguez",
            "email": "antonio@nobox.com"
        },
        {
            "name": "Jonathan Torres",
            "email": "jonathan@nobox.com"
        },
        {
            "name": "Waldemar Figueroa",
            "email": "waldemar@nobox.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "nobox/lazy-strings": "^5.0",
        "illuminate/support": "5.1.*",
        "illuminate/console": "5.1.*",
        "illuminate/filesystem": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Nobox\\LazyStrings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nobox\\LazyStrings\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.4"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
